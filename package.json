{
  "name": "rajoyipsum",
  "displayName": "rajoyIpsum",
  "description": "Lorem Ipsum de Rajoy",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:rajoyipsum.generateIpsum"
  ],
  "main": "./extension.js",
  "icon": "images/icon.png",
  "publisher": "MarcoOsorio",
  "repository": {
    "type": "git",
    "url": "https://github.com/Mosorior/rajoyipsum.git"
  },
  "contributes": {
    "keybindings": [
      {
        "command": "rajoyipsum.generateIpsum",
        "key": "ctrl+alt+i",
        "mac": "cmd+alt+i",
        "when": "editorTextFocus"
      }
    ],
    "snippets": [
      {
        "language": "html",
        "path": "./snippets/snippets.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/snippets.json"
      },
      {
        "language": "php",
        "path": "./snippets/snippets.json"
      },
      {
        "language": "plaintext",
        "path": "./snippets/snippets.json"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test",
    "compile": "webpack --mode production"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.85.0",
    "@vscode/test-cli": "^0.0.6",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "vsce": "^2.15.0"
  }
}
