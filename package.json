{
  "name": "rajoyipsum",
  "displayName": "rajoyIpsum",
  "description": "Lorem Ipsum de Rajoy",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:rajoyipsum.generateIpsum"
  ],
  "main": "./extension.js",
  "icon": "images/icon.png",
  "contributes": {
    "keybindings": [
        {
            "command": "rajoyipsum.generateIpsum",
            "key": "ctrl+alt+i",
            "mac": "cmd+alt+i",
            "when": "editorTextFocus"
        }
    ],
    "snippets": [{
      "language": "html",
      "path": "./snippets/snippets.json"
    },
    {
      "language": "javascript",
      "path": "./snippets/snippets.json"
    },
    {
      "language": "php",
      "path": "./snippets/snippets.json"
    },
    {
      "language": "plaintext",
      "path": "./snippets/snippets.json"
    }]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.6",
    "@vscode/test-electron": "^2.3.9"
  }
}
